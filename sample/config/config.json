{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "fast-serve-sample-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/fastServeSample/FastServeSampleWebPart.js",
          "manifest": "./src/webparts/fastServeSample/FastServeSampleWebPart.manifest.json"
        }
      ]
    },
    "fast-app-customizer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/fastAppCustomizer/FastAppCustomizerApplicationCustomizer.js",
          "manifest": "./src/extensions/fastAppCustomizer/FastAppCustomizerApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FastServeSampleWebPartStrings": "lib/webparts/fastServeSample/loc/{locale}.js",
    "FastAppCustomizerApplicationCustomizerStrings": "lib/extensions/fastAppCustomizer/loc/{locale}.js"
  }
}
